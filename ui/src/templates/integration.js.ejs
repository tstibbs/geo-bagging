function showError(errorMessage) {
    alert(errorMessage)
}
<%= require('raw-loader!./feature-detect.js').default %>

window.geoBaggingBaseUrl = /^(.*)\/integration\/trigpointing\.js.*$/.exec(document.currentScript.src)[1] + '/';
var defaultOptions = {
    dataType: "script",
    cache: true
};
<%= htmlWebpackPlugin
    .tags
    .headTags
    .map(tag =>
        `\$("head").append('<link rel="stylesheet" type="text/css" href="' + window.geoBaggingBaseUrl + '${(tag.attributes.href || tag.attributes.src).replace(/^..\//, '')}">');\n`
    )
%>
<%= htmlWebpackPlugin
    .tags
    .bodyTags
    .map(tag =>
        `$.ajax($.extend({url: window.geoBaggingBaseUrl + "${tag.attributes.src.replace(/^..\//, '')}"}, defaultOptions));\n`
    )
%>
